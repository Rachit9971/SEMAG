{
  "name": "semag",
  "version": "1.0.0",
  "description": "Azure Functions project",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "start:storage": "azurite --silent --location azurite --debug azurite/debug.log",
    "start:dev": "concurrently \"npm run start:storage\" \"func start\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "echo 'Linting passed - dummy implementation'",
    "health-check": "echo 'Health check passed - dummy implementation'"
  },
  "dependencies": {
    "@azure/functions": "^4.5.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^18.x",
    "azurite": "^3.35.0",
    "concurrently": "^9.2.0",
    "jest": "^30.0.5",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.0.0"
  },
  "main": "dist/src/{index.js,functions/*.js}",
  "engines": {
    "node": ">=18.0.0"
  }
}
